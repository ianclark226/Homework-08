<div class="card">
	<h5 class="card-header">
		<%= member.getName() %>
		Intern
	</h5>
	<div class="card-body">
		<ul class="list-group list-group-flush">
			<li class="list-group-item">Id: <%= member.getId() %></li>
			<li class="list-group-item">Email: <%= member.getEmail() %></li>
			<li class="list-group-item">School: <%= member.getSchool() %></li>
		</ul>
	</div>
</div>